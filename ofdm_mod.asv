function [frames,c] = ofdm_mod(qamsig,N,P)
   %N is the frame size
   qamstep = N/2-1;
   disp(qamstep)
   frames = zeros(N,1);
   
for i = 1:N/2-1:P*(N/2-1)
    disp(i)
%     a = qamsig(i:i+N/2-2);
%     disp(a)
%     b = flip(conj(qamsig(i:i+N/2-2)));
%     disp(b)
    frame_i = ifft([0;qamsig(i:i+N/2-2);0;flip(conj(qamsig(i:i+N/2-2)))]); %workhorse of the function
    disp(c)
    ofdmSeq = 0;
    frames = []
    
end
    
    frames(:,1) = [];
end


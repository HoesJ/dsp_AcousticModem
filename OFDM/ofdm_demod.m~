function [qamsig] = ofdm_demod(ofdm_seq,N,L)
    % reshape ofdm_sig
    qamsig = reshape(ofdm_seq, [N+L P]);
    
    % throw away cyclic prefix
    qamsig = qamsig(L+1:end,:);
    
    % fft
    qamsig = fft(qamsig);
    
    % throw away copies
    qamsig = qamsig(2:N/2,:);
    
    % reshape to a line
    qamsig = reshape(qamsig, [(N/2 - 1) * P 1]);
%    %N is the frame size
%    qamstep = N/2-1;
%    qamsig = [];
%    
% for i = 1:P
%     frame = fft(ofdm_seq(:,i));
%     qamsig = [qamsig;frame(2:qamstep+1,1)];
%     
% end
    
end
